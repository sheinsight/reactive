{
  "name": "reactive-mono-repo",
  "private": true,
  "version": "0.2.3",
  "packageManager": "pnpm@9.14.4",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm i && pnpm run build && pnpm -r --parallel dev",
    "build": "pnpm --filter '@shined/*' build",
    "test": "pnpm --filter '@shined/*' test",
    "test:ci": "pnpm --filter '@shined/*' test:ci",
    "lint": "biome check",
    "lint:fix": "biome check --fix --unsafe",
    "release": "bumpp -r",
    "docs:dev": "rspress dev docs",
    "docs:build": "pnpm install && esno ./scripts/build-docs.ts",
    "docs:release": "esno ./scripts/release-docs.ts",
    "docs:preview": "rspress preview docs"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rspress/shared": "1.28.0",
    "@types/node": "^20.17.9",
    "@types/semver": "^7.5.8",
    "@unocss/reset": "^0.62.4",
    "bumpp": "^9.8.1",
    "enquirer": "^2.4.1",
    "esno": "^4.8.0",
    "execa": "^8.0.1",
    "find-packages": "^10.0.4",
    "happy-dom": "^13.10.1",
    "less": "^4.2.1",
    "read-pkg": "^9.0.1",
    "read-yaml-file": "^2.1.0",
    "rsbuild-plugin-google-analytics": "^1.0.3",
    "rspress": "1.28.0",
    "semver": "^7.6.3",
    "tsx": "^4.19.2",
    "vitepress": "^1.5.0",
    "write-package": "^7.1.0",
    "zx": "^8.2.4"
  }
}
