{
  "name": "reactive",
  "version": "0.0.19",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.3",
  "license": "MIT",
  "author": {
    "name": "ityuany"
  },
  "scripts": {
    "build": "pnpm --filter '@shined/*' build ",
    "test": "pnpm  --filter '@shined/*' test",
    "test:w": "pnpm  --filter '@shined/*' test:w",
    "dev": "pnpm i && pnpm -r --parallel dev",
    "lint": "pnpm -r --parallel lint",
    "lint:root": "prettier --write ./examples/**/*.{ts,tsx,md} ./scripts ./readme.md --no-error-on-unmatched-pattern",
    "bump": "tsx ./scripts/bump.ts",
    "release": "tsx ./scripts/release.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@types/semver": "^7.5.8",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "find-packages": "^10.0.4",
    "happy-dom": "^13.6.0",
    "read-pkg": "^9.0.1",
    "read-yaml-file": "^2.1.0",
    "semver": "^7.6.0",
    "tsx": "^4.7.1",
    "write-package": "^7.0.0"
  }
}
